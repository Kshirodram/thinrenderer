			<ssi data-component-url="./components/stamp" data-component-name="stamp" data-content-model="http://localhost:3000/api/getsinglestamp" data-render-path="stamp" isServer="true">
			</ssi>

			<ssi data-component-url="./components/stamp" data-component-name="stamp" data-content-model="http://localhost:3000/api/getdoublestamp" data-render-path="stamp2" isServer="true">
			</ssi>

			<ssi data-component-url="./components/footer" data-component-name="footer" data-content-model="http://localhost:3000/api/getfooter" data-render-path="footer" isServer="true">
			</ssi>

