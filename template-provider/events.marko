	<body>
		<div class="root-level-loader">
			<ssi data-component-url="./components/header" data-component-name="header" data-content-model="http://localhost:3000/api/getheader" data-render-path="header" isServer="true">
			</ssi>

			<ssi data-component-url="./components/hero_banner" data-component-name="hero_banner" data-content-model="http://localhost:3000/api/getherobanner" data-render-path="hero_banner" isServer="true">
